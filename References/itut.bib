@report{ITU-TSTUDYGROUP12.SG12-C.22,
  abstract = {This investigation describes a P.800 ACR LOT for the subjective evaluation of EVS in an experimental design as used for the P.863 validation but going up to fullband (FB). 
P.863 shows an excellent prediction performance for EVS processed speech from narrowband to fullband. The experiment also contains Opus codec conditions (as used in OTT VoLTE services) and legacy codecs in mobile networks as AMR and AMR-WB. P.863 predicts the subject scores reliably, without significant bias for any coding scheme.
The experimental design was following the so-called P.OLQA full-scale rules, but extended to mixed fullband. The P.863 predictions follow this scale interpretation very nicely and linear. There is no mapping required.},
  author = {{ITU-T Study Group 12}},
  date = {2017-01-19},
  institution = {{International Telecommunication Union (Telecommunication Standardization Sector)}},
  number = {SG12-C.22},
  title = {A Subjective {{ACR LOT}} Testing Fullband Speech Coding and Prediction by {{P}}.863},
  type = {Contribution},
  url = {https://www.itu.int/md/T17-SG12-C-0022/en},
  urldate = {2018-04-20}
}


