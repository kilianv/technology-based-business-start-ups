@misc{digrasoftugMeineGPSPosition2020,
  title = {{Meine GPS Position -- Apps bei Google Play}},
  author = {{digrasoft UG}},
  year = {2020},
  month = nov,
  urldate = {2020-11-30},
  abstract = {Meine GPS Position zeigt dir die beste verf{\"u}gbare Position basierend auf Geolokationsdaten von allen aktuell verf{\"u}gbaren Standortanbietern wie GPS oder WLAN. Die App ist ideal f{\"u}r alle Aktivit{\"a}ten, die GPS-Koordinaten in Echtzeit ben{\"o}tigen, wie z.B. Geocaching oder Segeln. Meine GPS Position ist in drei Tabs gegliedert: Der {\"U}BERSICHT Tab zeigt detaillierte Informationen von den Standortsensoren in Echtzeit: Breitengrad und L{\"a}ngengrad, H{\"o}he, Genauigkeit, Geschwindigkeit und Bewegungsrichtung. F{\"u}r die Darstellung der Koordinaten stehen verschiedene Formate, z.B. Dezimalgrad oder UTM (Universal Transverse Mercator), zur Verf{\"u}gung. L{\"a}ngenangaben k{\"o}nnen in Metern oder Fu{\ss} angezeigt werden. F{\"u}r Geschwindigkeiten werden die Einheiten m/s, ft/s, km/h, mph und kn (Knoten) unterst{\"u}tzt. In der Ansicht KARTE kannst du einfach entdecken, was sich in deiner Umgebung befindet und Orte sehen, die du gespeichert hast, z.B. den tollen Strand vom deinem letzten Urlaub oder einfach nur die Stelle wo dein Auto parkt. Durch langes Dr{\"u}cken auf der Karte kannst du neue Orte hinzuf{\"u}gen. Um einen bereits gespeicherten Ort zu verschieben, halte ihn lange gedr{\"u}ckt und ziehe ihn an die neue Position. Alle g{\"a}ngigen Kartentypen wie Stra{\ss}enkarte oder Satellit werden unterst{\"u}tzt. Im Tab ORTE kannst du deine Lieblingsorte speichern, bearbeiten und l{\"o}schen und sehen, wie weit sie von deinem aktuellen Standort entfernt sind. Das ist sehr n{\"u}tzlich wenn du auf Reisen bist und die aktuelle Entfernung nach Hause wissen m{\"o}chtest. Zur Berechnung der Entfernung wird das WGS84 Ellipsoid verwendet. Von {\"u}berall in der App kannst du deine aktuelle Position {\"u}ber deinen Lieblingsmessenger teilen. Deine Freunde erhalten deine GPS Koordinaten und einen Link zu Google Maps mit deiner Position. Im Notfall kannst du mit dieser App eine SMS mit deinem GPS-Standort schicken, selbst wenn keine Internetverbindung verf{\"u}gbar ist. Zus{\"a}tzlich kannst du deine GPS-Koordinaten in die Zwischenablage kopieren und sie in Karten-Apps, Chats oder E-Mails einf{\"u}gen.},
  howpublished = {https://play.google.com/store/apps/details?id=com.digrasoft.mygpslocation\&hl=de\&gl=US},
  langid = {ngerman},
  file = {C:\Users\Kilian\Zotero\storage\UQHSN2LX\details.html}
}

